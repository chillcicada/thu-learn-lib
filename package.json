{
  "name": "thu-learn-lib",
  "type": "module",
  "version": "3.1.4",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "author": {
    "name": "Harry Chen",
    "email": "harry-chen@outlook.com",
    "url": "https://harrychen.xyz"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "bugs": {
    "url": "https://github.com/Harry-Chen/thu-learn-lib/issues"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "module": "lib/index.js",
  "unpkg": "lib/index.global.js",
  "jsdelivr": "lib/index.global.js",
  "types": "lib/index.d.ts",
  "browser": "lib/index.global.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest",
    "compile": "tsc --noEmit",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "entities": "4.5.0",
    "fetch-cookie": "3.0.1",
    "js-base64": "3.7.7",
    "node-fetch-cookie-native": "0.0.2",
    "node-fetch-native": "1.6.4",
    "tough-cookie": "4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.11.6",
    "@types/node": "20.11.30",
    "@types/tough-cookie": "4.0.5",
    "domhandler": "5.0.3",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-plugin-format": "0.1.0",
    "fake-parse5": "0.0.1",
    "lint-staged": "15.2.2",
    "simple-git-hooks": "2.11.1",
    "terser": "5.30.0",
    "tsup": "8.0.2",
    "typescript": "5.4.3",
    "vitest": "1.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
